{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ultra.api.eosnation.io/:path*"
    },
    {
      "source": "/kucoin/:path*",
      "destination": "https://api.kucoin.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}